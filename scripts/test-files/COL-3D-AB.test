# ============================================================================
# COL-3D-AB.TEST
# Colliding flows test with standard alpha-beta artificial viscosity.
# ============================================================================

# First, set Makefile options for test
NDIM=3
PERIODIC=1
X_BOUNDARY=PERIODIC
Y_BOUNDARY=PERIODIC
Z_BOUNDARY=PERIODIC
SPH=STANDARD
KERNEL=M4TC
HFIND=NUMBER
HYDRO=1
THERMAL=ISOTHERMAL
ARTIFICIAL_VISCOSITY=AB
ARTIFICIAL_CONDUCTIVITY=0
SELF_GRAVITY=0
SINKS=0
TREE=BH
REORDER=PARTICLES
NEIGHBOURLISTS=PARTICLES
DIMENSIONLESS=1
CHECK_NEIB_TIMESTEPS=2


# Next set parameters for params.dat file
run_id=COL-3D-AB
in_file=ICCOL3D-MACH4-M4.df.dat
in_file_form=dragon_form
out_file_form=seren_form
restart=.FALSE.
sph_endtime=0.6
firstsnap=1.0
snaptime=1.0
courant_mult=0.2
accel_mult=0.2
nlevels=10
dt_fixed=0.6
periodic_min_x=-4.0
periodic_max_x=4.0
periodic_min_y=0.0
periodic_max_y=0.2
periodic_min_z=0.0
periodic_max_z=0.2
pp_gather=50
alpha=1.0
beta=2.0
isotemp=1.0


# Program name
PROG_NAME=seren
PROG_EX=seren


# Set some variables for graphing
ANALYTIC="COL.exact"
PLOT_PROG=gnuplot
PLOT_FILE="COL-3D-AB.gnuplot"

